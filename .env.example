# Cloudflare SpeedTest DDNS - Environment Configuration
# Copy this file to .env and fill in your actual values

# Cloudflare API Configuration
zone_id=your_zone_id_here
api_token=your_api_token_here

# Domain Configuration
host_name=your.domain.com
host_ip_max=2

# CloudflareSpeedTest Parameters
# -n: Number of latency test threads (default 200, max 1000)
# -dn: Number of download tests (default 10)
# -sl: Minimum speed threshold in MB/s (default 20)
# -tl: Maximum latency threshold in ms (default 100)
speedtest_para="-n 800 -dn 2 -sl 20 -tl 100"
